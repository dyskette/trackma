<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="TrackmaAccountRow" parent="AdwActionRow">
    <child>
      <object class="GtkGestureClick" id="click_gesture">
        <property name="button">1</property>
        <signal name="pressed" handler="_on_account_clicked" swapped="no" />
      </object>
    </child>
    <child type="prefix">
      <object class="GtkPicture" id="account_logo">
        <property name="margin-top">9</property>
        <property name="margin-bottom">9</property>
      </object>
    </child>
  </template>
</interface>
